{
    "name": "project-kap",
    "displayName": "project kap",
    "description": "",
    "version": "0.0.1",
    "publisher": "michael",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*",
        "onCommand:extension.openWebview"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.openWebview",
                "title": "Open KAP"
            }
        ],
        "configuration": {
            "title": "KAP Configuration",
            "properties": {
                "userId": {
                    "type": "string",
                    "default": "lisa",
                    "description": "Must matching user id in the chrome extension"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "copy-paste": "^1.3.0",
        "firebase": "^4.10.1",
        "lodash": "^4.17.5",
        "query-string": "^5.1.0",
        "uniqid": "^4.1.1"
    }
}

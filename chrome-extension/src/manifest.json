{
  "name": "Knowledge Accelerator for Programming",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentAnnotation.bundle.js"
      ],
      "css": [
        "content.styles.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "content.styles.css",
    "thumb-down-100.jpg",
    "thumb-up-100.jpg",
    "question-mark-100.jpg",
    "icon-128.png",
    "icon-inactive-128.png",
    "googlesans-regular.ttf"
  ],
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": [
    "downloads",
    "contextMenus",
    "tabs",
    "activeTab",
    "<all_urls>",
    "background"
  ],
  "key": "",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval' http://localhost:3000 http://localhost:3001 http://localhost:3002 https://gstatic.com https://*.google.com https://*.firebaseio.com https://cdn.firebase.com https://kap-project-nsh-2504.firebaseapp.com  https://project-kap-dev.firebaseapp.com; object-src 'self'"
}
